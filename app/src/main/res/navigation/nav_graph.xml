            android:id="@+id/action_notificationFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_notificationFragment_to_detailProdukFragment"
            app:destination="@id/detailProdukFragment" />
        <action
            android:id="@+id/action_notificationFragment_to_saleListFragment"
